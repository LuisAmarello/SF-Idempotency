<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ExtractVariables continueOnError="false" enabled="true" name="EV-Headers">
    <DisplayName>EV-Headers</DisplayName>
    <Header name="B-Application">
        <Pattern ignoreCase="true">{b-application}</Pattern>
    </Header>
    <Header name="Authentication-Code">
        <Pattern ignoreCase="true">{authentication-code}</Pattern>
    </Header>
    <IgnoreUnresolvedVariables>false</IgnoreUnresolvedVariables>
    <Source clearPayload="false">request</Source>
    <VariablePrefix>flow.request.headers</VariablePrefix>
</ExtractVariables>